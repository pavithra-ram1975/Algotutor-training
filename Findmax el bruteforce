public class max {
    public static void main(String[] args)
    {
        int[] arr = {12,80,8,5,1,2};
        // int sortarr;
        int max = arr[0];
        for(int i=0;i<=arr.length-1;i++){
            if(max<arr[i]){
                max=arr[i];
            }
            
        }
        System.out.println(max);

    }
    
}
